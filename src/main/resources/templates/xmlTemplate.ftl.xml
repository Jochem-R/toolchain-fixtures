<#function output item>
    <#if item?is_hash>
        <#assign obj = "">
        <#list item as k, v>
            <#assign obj += '<${k}>' + output(v) + "</${k}>">
        </#list>
    <#elseif item?is_sequence>
        <#assign obj = "">
        <#list item as v>
            <#assign obj += output(v)>
        </#list>
    <#else>
        <#assign obj = item>
    </#if>
    <#return obj>
</#function>
